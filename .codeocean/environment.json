{
	"version": 1,
	"base_image": "codeocean/mambaforge3:24.5.0-0-python3.12.4-ubuntu22.04",
	"args": [
		"AWS_ACCESS_KEY_ID",
		"AWS_DEFAULT_REGION",
		"AWS_SECRET_ACCESS_KEY"
	],
	"post_install": true,
	"options": {
		"registry_host_arg": true,
		"git_ask_pass": true
	},
	"installers": {
		"apt": {
			"packages": [
				{
					"name": "build-essential",
					"version": "12.9ubuntu3"
				}
			],
			"install_software_properties_common": true
		},
		"pip": {
			"packages": [
				{
					"name": "aind-data-schema",
					"version": "1.3.0"
				},
				{
					"name": "hdbscan",
					"version": "0.8.40"
				},
				{
					"name": "spikeinterface[full]",
					"version": "0.102.1"
				},
				{
					"name": "torch",
					"version": "2.6.0"
				}
			],
			"options": {},
			"pre_install_options": {}
		}
	}
}